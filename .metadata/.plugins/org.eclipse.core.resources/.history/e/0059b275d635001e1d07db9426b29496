package basic;

class Student{
	int id;
	String name;
	String address;
	long contact;
	static String cname="TOPS";
	Student(int id,String name,String address,long contact){
		this.id=id;
		this.name=name;
		this.address=address;
		this.contact=contact;
	}
	public void showdata() {
		System.out.println("id : "+id+" name : "+name+" address : "+address+" contact : "+contact+" cname : "+cname );
	}
}
public class P009_StaticKeyword {
	public static void main(String[] args) {
		
	}
}
