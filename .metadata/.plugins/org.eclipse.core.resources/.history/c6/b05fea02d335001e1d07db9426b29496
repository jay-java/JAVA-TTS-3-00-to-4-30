package basic;
//Constructor-> is special member function of class 
//				which has same name as class name
//			-> called automatically when object is created
//			-> have no return type
//			-> 3 types
//			->1.Default 2.Parameterized 3.Copy
class Data{
	Data(){
		System.out.println("default cons");
	}
	public void call() {
		System.out.println("call function in data class");
	}
}
public class P008_Constructor {
	public static void main(String[] args) {
		Data d = new Data();
		d.call();
	}
}
